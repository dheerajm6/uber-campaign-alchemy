# Cache control for favicon files
/favicon.ico
  Cache-Control: public, max-age=86400
/favicon.svg
  Cache-Control: public, max-age=86400
/favicon-*.png
  Cache-Control: public, max-age=86400

# Cache control for static assets
/*.png
  Cache-Control: public, max-age=31536000
/*.jpg
  Cache-Control: public, max-age=31536000
/*.svg
  Cache-Control: public, max-age=31536000